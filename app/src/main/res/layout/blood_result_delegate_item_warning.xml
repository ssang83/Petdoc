<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="viewModel"
            type="kr.co.petdoc.petdoc.viewmodel.MyHealthCareResultViewModel" />
        <variable
            name="item"
            type="kr.co.petdoc.petdoc.model.healthcareresult.BloodWarning" />
    </data>
    <kr.co.petdoc.petdoc.widget.BloodResultWarningView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:bloodWarning="@{item}"
        app:quickSearchCategory="@={viewModel.quickSearchBloodCategory}"/>

</layout>
